  </PropertyGroup>

  <ItemGroup>
    <None Remove="license.txt" />
    <None Remove="license.txt" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Helpers\" />
    <Folder Include="Diagrams\" />
    <Folder Include="Services\" />
    <Folder Include="Patterns\" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="LUMO.Model">
      <HintPath>..\..\LUMO-Model\LUMO.Model\bin\Release\netstandard2.0\LUMO.Model.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
